@page
@model LitterReportModel;
@{
    ViewData["Title"] = "American Bully Kennel Club";
}
<h1 class="display-4">Test project for PDFSharp task</h1>
<div class="text-left"> 
 
</div>

<div class="text-right">        
  <div class="input-group">
    <form method="POST">
        <div class="text-left">
        <br>
        <label>First page</label>
        <div>Litter Number<input asp-for="LitterReport.LitterNumber" /></div>
        <div>Sire name<input asp-for="LitterReport.SireName" /></div>
        <div>Dam Name<input asp-for="LitterReport.DamName" /></div>
        <div>Report date<input asp-for="LitterReport.ReportGenerationDate" /></div>
        <div>Birthdate<input asp-for="LitterReport.Birthdate" /></div>
        <div>Breed<input asp-for="LitterReport.Breed" /></div>
        <input type="submit" />
    </form>
</div>